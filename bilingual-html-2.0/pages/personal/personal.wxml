
<!--personal.wxml-->
<view class="containerPersonal" bindtap="toBabyInfo">
    <image src="data:image/png;base64," class="pencil"></image>
  <view class="info" >
    <view class="headPic">
      <image src="{{imgUrl}}" mode="aspectFill"></image>
    </view>
    <!--宝宝资料完善视图-->
    <view class="bbInfo" wx:if="{{user.infoStatus===1}}">
      <view class="bbName">{{user.name}}</view>
      <view class="babymessage">
        <!--数据来了改回来-->
        <text class="bbBrith" wx:if="{{user.age>=12}}">宝宝年龄：{{(user.showage)}}岁</text>
        <text class="bbBrith" wx:if="{{user.age<12}}">宝宝年龄：{{(user.age)}}月</text>
        <text class="bbSex">宝宝性别：{{user.gender}}</text>
      </view>
    </view>
    <!--宝宝资料未完善视图-->
    
     <view class="bbInfoTip" wx:if="{{user.infoStatus===0}}">
      <view>完善宝宝的个人信息</view>
      <view>让身边的老师和小朋友更容易记住你~</view>
    </view>
    
  </view>

  <!--<view class="line"></view>-->
</view>
<view class="card-Record" bindtap="toCardRecord">
  
  <text>我的打卡记录</text>
  <image src="data:image/png;base64," class="nextIcon" style="margin-right:6rpx"></image>
</view>
<view class="aboutOur" bindtap="toAboutOur">
 
  <text>关于我们</text>
  <image src="data:image/png;base64," class="nextIcon" style="width:40rpx;"></image>
</view>
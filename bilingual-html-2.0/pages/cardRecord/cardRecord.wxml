
<!--cardRecord.wxml-->
<view class="container">
  <view class="chart" wx:if="{{total!=0}}" >
    <view class="chart-duration">时长<text>(s)</text>
    </view>
    <view class="total-card">累计打卡<text>{{totalRecord}}</text>天
    </view>
    <!--<image src="../../images/record.png" style="width:100%"></image>-->
    <canvas wx:if="{{total!=0}}" canvas-id="myCanvas" style="width:100%;height:173px;"/>
    <view class="tips"><image src="data:image/png;base64,"></image>
    <text wx:if="{{!isShare}}">图表展示的是你打卡的记录哦，继续加油呦~~</text>
    <text wx:if="{{isShare}}">图表展示的是{{shareName}}小朋友的记录哦。</text>
    </view>
  </view>
  <view class="middle" wx:if="{{total!=0}}" ></view>
  <view class="cardRecord">
    <view wx:if="{{total==0}}" class="noCardRecordList">
      <view class="noRecord">
        <image src="data:image/png;base64,"></image>
      </view>
      <view class="noRecordTips">
        <view>您还没打过卡哦</view>
        <view>快去体验一下吧</view>
      </view>
    </view>
    <view wx:if="{{total>0}}" wx:for="{{list}}" bindtap="toCardInfo" data-id="{{item.id}}" class="cardRecordList">
      <view class="play" style="background-image:url({{headPic}});" data-id="{{item.id}}">
   <!--这里先写死，等数据来了再改活-->
        <image src="data:image/png;base64," style="opacity:0.8" data-id="{{item.id}}" ></image>
      </view>
      <view class="cardInfo" data-id="{{item.id}}">
        <view class="cid" data-id="{{item.id}}">
            <text class="cid" data-id="{{item.id}}">{{item.recordName}}</text>        
        </view>
            <text class="duration" data-id="{{item.id}}">{{item.recordSeconds}}s</text>
            <text class="createAt" data-id="{{item.id}}">{{item.updateAt}}</text>
      </view>
    </view>
  </view>
</view>
/**cardInfo.wxss**/
.audio {
    height: 166rpx;
    border: 1px solid #e8e8e8;
    position: relative;
    display: flex;
    flex-flow: row wrap;
    align-items: center;
    width: 91%;
    margin: 0 auto;
    border-radius: 5px;
}
.audio image{
    width: 66rpx;
    height:66rpx;
    margin-left: 26rpx;
}
.sideBar{
    position: relative;
    margin:20rpx;
}
.progress{   
    width:490rpx;
}
.circle{
    background:transparent ;
    position: absolute;
   transform: translate(-10rpx, -18rpx);
    display: inline-block;
    width:31rpx;
    height: 31rpx;
    background:url('data:image/png;base64,');
    background-size:cover;
}
.start ,.end{
    color: #999999;
    font-size: 24rpx;
}
.start{
    margin: 0 20rpx;

}
.mainbody{
    width: 92%;
    /*height: 700rpx;*/
    /*border:1px solid black;*/
    /*margin-top:50rpx;*/
    padding-bottom:235rpx;
    margin-left:30rpx;
}
.title{
    line-height: 20rpx;
    width:95%;
    padding-left:30rpx;
}
.title-content{
    font-size: 32rpx;
    color: #333333;
    line-height: 50rpx;
}
.time{
   font-size: 24rpx;
   color: #999999;
   float: right;
   padding-right: 40rpx;
   margin-bottom: 40rpx;
}
.share image{
    position: fixed;
    bottom: 0;
  width: 750rpx;
  height: 190rpx;
}
.tip{
    margin-top: 55rpx;
    font-size: 30rpx;
    color: #000000;
      
}
.tip image{
    width: 35rpx;
    height:28rpx;
    vertical-align: -8%;
    margin-right: 10rpx;
}
.content{
    width: 95%;
    margin-top: 40rpx;
    font-size: 28rpx;
    word-break: break-all;
    line-height: 30rpx;

}
.content text:first-child{
    margin-right: 40rpx;
}
.word{
    margin-bottom: 40rpx;
}
.chinese{
    color: #999999;
    margin-bottom: 33rpx;
    font-size: 26rpx;
}
.english{
    margin-bottom: 15rpx;
    font-size: 28rpx;
}
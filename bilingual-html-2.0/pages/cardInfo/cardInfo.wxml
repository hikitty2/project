
<!--cardInfo.wxml-->
<view style="background:white;min-height:900rpx;">
<view class="title">
    <text class="title-content">
      {{record.recordName}}
    </text>
    <text class="time">
      {{record.updateAt}}
    </text>
  </view>
<view class="audio" >
  <image wx:if="{{playIcon==true}}" src="data:image/png;base64,"
  bindtap="goPlay">
  </image>
  <image wx:elif="{{playIcon==false}}" src="data:image/png;base64," bindtap="goPlay"></image>
  <view class="sideBar">
    <progress class="progress" percent="{{percent}}" stroke-width="4" color="#5fc35c" />
    <text class="circle" style="left:{{percent}}%"></text>
  </view>
  <view class="end">{{end}}s</view>
</view>
<view class="mainbody">
  <view class="tip"><image src="data:image/png;base64,"></image>快来看看今日学习句型:</view>
  <view class="content">
    <view wx:for="{{recordContent}}" class="word">
      <view class="english">{{item.en}}</view>
      <view class="chinese">{{item.ch}}</view>
    </view>
  </view>
</view>

</view>
 